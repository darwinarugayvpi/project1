ID}`, {\n      method: 'PUT',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\n        firstName: this.state.firstName,\n        middleName: this.state.middleName,\n        lastName: this.state.lastName,\n        gender: this.state.gender,\n        dateOfBirth: this.state.dateOfBirth,\n      }),\n    })\n      .then((res) => res.json())\n      .then((data) => {\n        console.log(data);\n      });\n  };\n\n  render() {\n    const dateFormat = 'YYYY/MM/DD';\n    // console.log(this.state.dateOfBirth);\n    return (\n      <div className=\"edit-page\">\n        <h2>Edit Information</h2>\n        <div className=\"edit-page-form\">\n          <form onSubmit={this.handleSubmit}>\n            <FormInput\n              value={this.state.firstName}\n              name=\"firstName\"\n              handleChange={this.handleChange}\n              label=\"First Name\"\n            />\n            <FormInput\n              value={this.state.middleName}\n              name=\"middleName\"\n              handleChange={this.handleChange}\n              label=\"Middle Name\"\n            />\n            <FormInput\n              value={this.state.lastName}\n              name=\"lastName\"\n              handleChange={this.handleChange}\n              label=\"Last Name\"\n            />\n            <label>\n              Gender:\n              <FormRadio\n                id=\"male\"\n                value=\"male\"\n                checked={this.state.gender === 'male' ? true : false}\n                handleChange={this.handleChange}\n              />\n              <FormRadio\n                id=\"female\"\n                value=\"female\"\n                checked={this.state.gender === 'female' ? true : false}\n                handleChange={this.handleChange}\n              />\n            </label>\n            {this.state.dateOfBirth === '' || this.state.hidden ? (\n              <React.Fragment>\n                <p>\n                  No birth date, do you want to add?{' '}\n                  <a\n                    style={{ textDecoration: 'underline' }}\n                    onClick={() =>\n                      this.setState({ hidden: !this.state.hidden })\n                    }\n                  >\n                    Click here.\n                  </a>{' '}\n                </p>\n              </React.Fragment>\n            ) : (\n              <label>\n                Date of Birth:\n                <DatePicker\n                  onChange={this.handleDate}\n                  name=\"dateOfBirth\"\n                  showToday={false}\n                  defaultValue={moment(`${this.state.dateOfBirth}`, dateFormat)}\n                  disabledDate={(current) => {\n                    return (\n                      moment().add(-18, 'years') <= current ||\n                      moment().add(-60, 'years') >= current\n                    );\n                  }}\n                />\n              </label>\n            )}\n\n            {this.state.hidden ? (\n              <div>\n                <label>\n                  Date of Birth:\n                  <DatePicker\n                    onChange={this.handleDate}\n                    name=\"dateOfBirth\"\n                    showToday={false}\n                    defaultValue={(current) => {\n                      return moment(\n                        `${new Date().getFullYear() - 18}`,\n                        dateFormat\n                      );\n                    }}\n                    disabledDate={(current) => {\n                      return (\n                        moment().add(-18, 'years') <= current ||\n                        moment().add(-60, 'years') >= current\n                      );\n                    }}\n                  />\n                </label>\n              </div>\n            ) : null}\n\n            <Button type=\"submit\">Update</Button>\n          </form>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default EditPage;\n","/home/darwin/vpi/project1/front-end/src/components/table/Table.jsx",[],"/home/darwin/vpi/project1/front-end/src/components/form-date/FormDate.jsx",[],"/home/darwin/vpi/project1/front-end/src/components/form-radio/FormRadio.jsx",[],"/home/darwin/vpi/project1/front-end/src/components/form-input/FormInput.jsx",[],"/home/darwin/vpi/project1/front-end/src/components/button/Button.jsx",[],"/home/darwin/vpi/project1/front-end/src/components/table/table-head/TableHead.jsx",["69"],"import React from 'react';\n\nconst TableHead = ({ tableHead }) => {\n  return (\n    <thead>\n      <tr scope=\"col\">\n        {tableHead.length === 0 ? (\n          <th style={{ textAlign: 'center' }}>No record.</th>\n        ) : (\n          tableHead\n            .filter((list) => list !== '__v')\n            .map((list) => {\n              return <th key={list}>{list}</th>;\n            })\n        )}\n        {tableHead.length !== 0 ? <th>Actions</th> : null}\n      </tr>\n    </thead>\n  );\n};\n\nexport default TableHead;\n","/home/darwin/vpi/project1/front-end/src/components/table/table-body/TableBody.jsx",["70"],"import React, { Component } from 'react';\nimport { Link } from 'react-router-dom';\nimport DeleteIcon from '@material-ui/icons/Delete';\nimport EditIcon from '@material-ui/icons/Edit';\n\nimport Button from '../../button/Button';\n\nclass TableBody extends Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {};\n  }\n  // componentDidUpdate(prevProps, prevState) {\n  //   if (prevProps.tableBody !== this.props.tableBody) {\n  //     console.log(`not equal`);\n  //   }\n  // }\n  render() {\n    // console.log(this.props);\n    return (\n      <tbody>\n        {this.props.tableBody.map((user) => {\n          return (\n            <tr scope=\"row\">\n              <td>{user._id}</td>\n              <td>{user.firstName}</td>\n              <td>{user.middleName}</td>\n              <td>{user.lastName}</td>\n              <td>{user.gender}<